    "def solution(s):\n",
    "    p=[]\n",
    "    y=[]\n",
    "    for char in s:\n",
    "        if char.lower()=='p':\n",
    "            p.append(char)\n",
    "        elif char.lower()=='y':\n",
    "            y.append(char)\n",
    "    answer=len(p)==len(y)\n",
    "    return answer\n",
    "\n",
    "\n",
    "solution(\"pPoooyY\")\n",
    "solution(\"Pyy\")"
   ]
